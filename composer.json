{
    "name": "kalnoy/nestedset",
    "description": "Nested Set Model for Laravel 4-5",
    "keywords": ["laravel", "nested sets", "nsm", "database", "hierarchy"],
    "license": "MIT",

    "authors": [
        {
            "name": "Alexander Kalnoy",
            "email": "lazychaser@gmail.com"
        }
    ],

    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.0 - 5.5",
        "illuminate/database": "5.0 - 5.5",
        "illuminate/events": "5.0 - 5.5"
    },

    "autoload": {
        "psr-4": {
            "Kalnoy\\Nestedset\\": "src/"
        }
    },

    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },

    "minimum-stability": "dev",

    "extra": {
        "branch-alias": {
            "dev-master": "v4.2.x-dev"
        }
    }
}
